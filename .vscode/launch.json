{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch 1.html",
            "file": "${workspaceFolder}/1.html"
        },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/1.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal"
        },
        {
            "name": "d1 Debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Debug/cppdebug1.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "CMake: build cpp1"
        }
    ]
}